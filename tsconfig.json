{
  "compilerOptions": {
    "lib": ["ES2016"],
    "target": "ES2016",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "paths": {
      "@teamkeel/sdk": ["./.build/sdk"],
      "@teamkeel/testing": ["./.build/testing"]
    },
    "skipLibCheck": true,
    "strictNullChecks": true,
    "types": ["node"],
    "allowJs": true,
    "resolveJsonModule": true
  },
  "include": ["**/*.ts"],
  "exclude": ["node_modules"]
}
